<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_nav"
    app:startDestination="@id/permissionsFragment">
    <fragment
        android:id="@+id/cameraFragment"
        android:name="com.example.healthc.presentation.home.camera.CameraFragment"
        android:label="fragment_camera"
        tools:layout="@layout/fragment_camera" >
        <action
            android:id="@+id/action_cameraFragment_to_textDetectionFragment"
            app:destination="@id/textDetectionFragment" >
            <argument
                android:name="imageUrl"
                app:argType="string"/>
            <argument
                android:name="language"
                app:argType="string"/>
        </action>
        <action
            android:id="@+id/action_cameraFragment_to_profileFragment"
            app:destination="@id/profileFragment" />
        <action
            android:id="@+id/action_cameraFragment_to_recipeSearchFragment"
            app:destination="@id/recipeSearchFragment" />
        <action
            android:id="@+id/action_cameraFragment_to_objectDetectionFragment"
            app:destination="@id/objectDetectionFragment" >
            <argument
                android:name="imageUrl"
                app:argType="string"/>
        </action>
        <action
            android:id="@+id/action_cameraFragment_to_productSearchFragment"
            app:destination="@id/productSearchFragment" />
        <action
            android:id="@+id/action_cameraFragment_to_korProductFragment"
            app:destination="@id/korProductFragment" />
    </fragment>
    <fragment
        android:id="@+id/permissionsFragment"
        android:name="com.example.healthc.presentation.home.camera.PermissionsFragment"
        android:label="PermissionsFragment" >
        <action
            android:id="@+id/action_permissionsFragment_to_cameraFragment"
            app:destination="@id/cameraFragment" />
    </fragment>
    <fragment
        android:id="@+id/textDetectionFragment"
        android:name="com.example.healthc.presentation.ml.text_detection.TextDetectionFragment"
        android:label="TextDetectionFragment"
        tools:layout="@layout/fragment_text_detection" >
        <argument
            android:name="imageUrl"
            app:argType="string"/>
        <argument
            android:name="language"
            app:argType="string"/>
        <action
            android:id="@+id/action_textDetectionFragment_to_cameraFragment"
            app:destination="@id/cameraFragment" />
    </fragment>
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.example.healthc.presentation.profile.ProfileFragment"
        android:label="ProfileFragment"
        tools:layout="@layout/fragment_profile" >
    <action
            android:id="@+id/action_profileFragment_to_profileAllergyFragment"
            app:destination="@id/profileAllergyFragment" />
        <action
            android:id="@+id/action_profileFragment_to_cameraFragment"
            app:destination="@id/cameraFragment" />
    </fragment>
    <fragment
        android:id="@+id/profileAllergyFragment"
        android:name="com.example.healthc.presentation.profile.profile_allergy.ProfileAllergyFragment"
        android:label="ProfileAllergyFragment"
        tools:layout="@layout/fragment_profile_allergy" >
    <action
            android:id="@+id/action_profileAllergyFragment_to_profileFragment"
            app:destination="@id/profileFragment" />
    </fragment>
    <fragment
        android:id="@+id/recipeSearchFragment"
        android:name="com.example.healthc.presentation.allergy_information.recipe.RecipeSearchFragment"
        android:label="RecipeSearchFragment"
        tools:layout="@layout/fragment_recipe_search" >
        <action
            android:id="@+id/action_recipeSearchFragment_to_recipeFragment"
            app:destination="@id/recipeFragment">
            <argument
                android:name="allergy"
                app:argType="string"/>
        </action>
        <action
            android:id="@+id/action_recipeFragment_to_cameraFragment"
            app:destination="@id/cameraFragment" />
    </fragment>
    <fragment
        android:id="@+id/recipeFragment"
        android:name="com.example.healthc.presentation.allergy_information.recipe.RecipeFragment"
        android:label="RecipeFragment"
        tools:layout="@layout/fragment_recipe" >
        <argument
            android:name="allergy"
            app:argType="string"/>
    <action
            android:id="@+id/action_recipeFragment_to_recipeSearchFragment"
            app:destination="@id/recipeSearchFragment" />
    </fragment>
    <fragment
        android:id="@+id/objectDetectionFragment"
        android:name="com.example.healthc.presentation.ml.object_detection.ObjectDetectionFragment"
        android:label="ObjectDetectionFragment"
        tools:layout="@layout/fragment_object_detection" >
        <argument
            android:name="imageUrl"
            app:argType="string"/>
        <action
            android:id="@+id/action_objectDetectionFragment_to_cameraFragment"
            app:destination="@id/cameraFragment" />
    </fragment>
    <fragment
        android:id="@+id/productSearchFragment"
        android:name="com.example.healthc.presentation.allergy_information.product.product_search.ProductSearchFragment"
        android:label="ProductSearchFragment"
        tools:layout="@layout/fragment_product_search" >
        <action
            android:id="@+id/action_productSearchFragment_to_productFragment"
            app:destination="@id/productFragment" >
            <argument
                android:name="productId"
                app:argType="integer"/>
        </action>
        <action
            android:id="@+id/action_productSearchFragment_to_cameraFragment"
            app:destination="@id/cameraFragment" />
    </fragment>
    <fragment
        android:id="@+id/productFragment"
        android:name="com.example.healthc.presentation.allergy_information.product.ProductFragment"
        android:label="ProductFragment"
        tools:layout="@layout/fragment_product" >
        <action
            android:id="@+id/action_productFragment_to_productSearchFragment"
            app:destination="@id/productSearchFragment" />
        <argument
            android:name="productId"
            app:argType="integer"/>
    </fragment>
    <fragment
        android:id="@+id/korProductFragment"
        android:name="com.example.healthc.presentation.allergy_information.kor_product.KorProductFragment"
        android:label="KorProductFragment"
        tools:layout="@layout/fragment_kor_product" >
        <action
            android:id="@+id/action_korProductFragment_to_cameraFragment"
            app:destination="@id/cameraFragment" />
    </fragment>
</navigation>